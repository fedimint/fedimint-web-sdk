{
  "extends": "../tsconfig.base.json",
  "compilerOptions": {
    "module": "ESNext",
    "moduleResolution": "Node",
    "allowSyntheticDefaultImports": true,
    "strict": false,
    "noEmit": true,
    "skipLibCheck": true,
    "resolveJsonModule": true,
    "allowJs": true,
    "types": ["vitepress/client"],
    "jsx": "preserve"
  },
  "include": [".vitepress/**/*", "**/*.md", "**/*.vue"],
  "exclude": ["node_modules", ".vitepress/dist", ".vitepress/cache"]
}
